{
  "name": "nexuscript-twilio-sms-action",
  "version": "1.0.0",
  "description": "Send SMS messages via twilio",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production"
  },
  "author": "nexusoft",
  "license": "ISC",
  "devDependencies": {
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "nexusdk": "github:wizebin/nexusdk",
    "react-native-digest-fetch": "^1.0.4",
    "twilio": "^3.17.0"
  },
  "nexuscript": {
    "type": "action",
    "author": "nexuscript",
    "price": "free",
